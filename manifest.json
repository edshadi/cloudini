{
  "name": "Cloudini Flux",
  "version": "0.1",
  "description": "Project Mangement right in your gmail",
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": ["js/bundle.js"],
      "css": ["css/app.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/"
  ],
  "manifest_version" : 2,
  "web_accessible_resources" : ["images/*"]
}
