{
  "name": "Cloudini Flux",
  "version": "0.1",
  "description": "Project Mangement right in your gmail",
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": ["js/bundle.js"],
      "css": ["css/app.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://script.google.com/*",
    "https://accounts.google.com/o/oauth2/token",
    "https://script.google.com/"
  ],
  "manifest_version" : 2,
  "web_accessible_resources" : ["images/*"]
}
